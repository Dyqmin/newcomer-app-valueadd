{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": ".",
  "projects": {
    "newcomer-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "apps/newcomer-app",
      "sourceRoot": "apps/newcomer-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/newcomer-app",
            "index": "apps/newcomer-app/src/index.html",
            "main": "apps/newcomer-app/src/main.ts",
            "polyfills": "apps/newcomer-app/src/polyfills.ts",
            "tsConfig": "apps/newcomer-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/newcomer-app/src/favicon.ico",
              "apps/newcomer-app/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/newcomer-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/newcomer-app/src/environments/environment.ts",
                  "with": "apps/newcomer-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "newcomer-app:build:production"
            },
            "development": {
              "browserTarget": "newcomer-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "newcomer-app:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/newcomer-app/jest.config.js",
            "tsConfig": "apps/newcomer-app/tsconfig.spec.json",
            "setupFile": "apps/newcomer-app/src/test.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "newcomer-app-recipe-data-access": {
      "projectType": "library",
      "root": "libs/newcomer-app/recipe/data-access",
      "sourceRoot": "libs/newcomer-app/recipe/data-access/src",
      "prefix": "newcomer-app",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/newcomer-app/recipe/data-access"],
          "options": {
            "jestConfig": "libs/newcomer-app/recipe/data-access/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/newcomer-app/recipe/data-access/src/**/*.ts",
              "libs/newcomer-app/recipe/data-access/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "newcomer-app-recipe-domain": {
      "projectType": "library",
      "root": "libs/newcomer-app/recipe/domain",
